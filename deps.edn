{:paths ["src" "resources"]
 :deps
 {com.lambdaisland/cli    {:mvn/version "0.24.97"}
  
  ;; HTTP
  http-kit/http-kit {:mvn/version "2.8.1"}
  org.clojars.askonomm/ruuter {:mvn/version "1.3.5"}

  ;; Formats and utilities
  cheshire/cheshire {:mvn/version "5.13.0"}
  }
 :mvn/repos {"clojars" {:url "https://clojars.org/repo"}}

 :aliases
 {:dev
  {:extra-paths ["dev"]
   :extra-deps  {org.clojure/tools.namespace {:mvn/version "1.5.0"}}}

  :example
  {:extra-paths ["examples"]
   :extra-deps {;; clj-http/clj-http {:mvn/version "3.13.1"}
                ;; org.clojure/data.json {:mvn/version "2.5.1"}
                cheshire/cheshire {:mvn/version "5.13.0"}
                }}

  :test
  {:extra-paths ["test"]
   :extra-deps  {org.clojure/tools.gitlibs {:mvn/version "2.6.206"}
                 io.github.cognitect-labs/test-runner {:git/tag "v0.5.1" :git/sha "dfb30dd"}}}}}
